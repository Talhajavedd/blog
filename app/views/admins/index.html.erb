<h1>Listing users</h1>
 <%= link_to 'New User', new_admin_path %>
<table>
  <tr>
    <th>Name</th>
    <th colspan="3"></th>
  </tr>
 
  <% @users.each do |user| %>
  <% unless user == current_user %>
    <tr>
      <td><%= user.username %></td>
      <td><%= link_to 'Show', admin_path(user) %></td>
      <td><%= link_to 'Destroy', admin_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>

    <%end%>

  <% end %>
</table>

